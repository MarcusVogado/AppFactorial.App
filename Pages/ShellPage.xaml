<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppFactorial.Pages.ShellPage"
             xmlns:local="clr-namespace:AppFactorial.Pages"
             Shell.FlyoutBehavior="Disabled"      
             >
    <TabBar>
        <ShellContent
            Title="Início"
            ContentTemplate="{DataTemplate local:HomePage}"
            Route="HomePage"/>
        <ShellContent
            Title="Relatórios"            
            ContentTemplate="{DataTemplate local:Report}"
            Route="Report"/>
    </TabBar>
        
     
</Shell>